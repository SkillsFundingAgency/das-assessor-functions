{
	"version": "2.0",
	"functionTimeout": "02:00:00",
	"extensions": {
		"queues": {
			"maxPollingInterval": "00:00:02",
			"visibilityTimeout": "00:00:30",
			"batchSize": 6,
			"maxDequeueCount": 5,
			"newBatchThreshold": 2
		}
	},
	"logging": {
		"fileLoggingMode": "always",
		"logLevel": {
			"default": "Information",
			"Host.Results": "Error",
			"Function": "Trace",
			"Host.Aggregator": "Trace"
		}
	}
}